{
  "name": "rindo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "npm run build && firebase deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "check-links": "remark --frail --use remark-validate-links .",
    "prettier": "npm run prettier.base -- --write",
    "prettier.base": "prettier --cache \"./**/*.(js|jsx|ts|tsx|json5|json)\"",
    "prettier.dry-run": "npm run prettier.base -- --list-different"
  },
  "dependencies": {
    "@docusaurus/core": "3.1.1",
    "@docusaurus/preset-classic": "3.1.1",
    "@docusaurus/remark-plugin-npm2yarn": "^3.1.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "docusaurus-lunr-search": "^3.3.2",
    "docusaurus-theme-github-codeblock": "^2.0.2",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.1.1",
    "@docusaurus/types": "3.1.1",
    "@familyjs/prettier-config": "^4.0.0",
    "prettier": "^3.2.2",
    "remark-cli": "^12.0.0",
    "remark-validate-links": "^13.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "prettier": "@familyjs/prettier-config",
  "volta": {
    "node": "20.13.1",
    "npm": "10.5.2"
  }
